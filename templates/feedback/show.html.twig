









{% extends 'base.html.twig' %}

{% block body %}
    <h1>Feedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feedback.id }}</td>
            </tr>
            
            <tr>
                <th>IdParticipant</th>
                <td>{{ feedback.idParticipant }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ feedback.note }}</td>
            </tr>
            <tr>
                <th>Remarque</th>
                <td>{{ feedback.remarque }}</td>
            </tr>
            <tr>
                <th>DateFeedback</th>
                <td>{{ feedback.dateFeedback ? feedback.dateFeedback|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_feedback_index') }}">back to list</a>

    <a href="{{ path('app_feedback_edit', {'id': feedback.id}) }}">edit</a>

    {{ include('feedback/_delete_form.html.twig') }}
{% endblock %}
